    BackgroundBlurColor: {r: 0.8588236, g: 1, b: 0.10980393, a: 0}
    characterName: "\u042D\u043D\u043D\u0438"
    cameraPosition: {x: -5, y: 0, z: -10}
    dialogueText: "\u0414\u043E\u0431\u0440\u044B\u0439 \u0434\u0435\u043D\u044C,
      \u044F \u043C\u043E\u0433\u0443 \u0432\u0430\u043C \u043F\u043E\u043C\u043E\u0447\u044C?"
  - leftCharacterAnim: 1
    rightCharacterAnim: 2
    BackgroundBlurColor: {r: 0.85860395, g: 1, b: 0.10849059, a: 0}
    characterName: "\u0427\u043E\u043F\u0443\u043A\u0430\u0439"
    cameraPosition: {x: 5, y: 0, z: -10}
    dialogueText: "\u0417\u0434\u0440\u0430\u0432\u0441\u0442\u0432\u0443\u0439\u0442\u0435,
      \u043C\u043E\u0436\u0435\u0442 \u0431\u044B\u0442\u044C \u0432\u044B \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u043B\u0438
      \u0437\u0434\u0435\u0441\u044C \u044D\u0442\u0438 \u043F\u0440\u0435\u0434\u043C\u0435\u0442\u044B
      \u0438\u0437 \u0441\u043F\u0438\u0441\u043A\u0430?"
  - leftCharacterAnim: 2
    rightCharacterAnim: 1
    BackgroundBlurColor: {r: 0.85860395, g: 1, b: 0.10849059, a: 0}
    characterName: "\u042D\u043D\u043D\u0438"
    cameraPosition: {x: -5, y: 0, z: -10}
    dialogueText: "\u041A \u0441\u043E\u0436\u0430\u043B\u0435\u043D\u0438\u044E
      \u043D\u0435\u0442, \u043D\u043E \u044F \u043C\u043E\u0433\u0443 \u0432\u0430\u043C
      \u043F\u0440\u0435\u0434\u043B\u043E\u0436\u0438\u0442\u044C \u043D\u0435\u0447\u0442\u043E
      \u043B\u0443\u0447\u0448\u0435\u0435!"
  - leftCharacterAnim: 1
    rightCharacterAnim: 2
    BackgroundBlurColor: {r: 0.85860395, g: 1, b: 0.10849059, a: 0}
    characterName: "\u0427\u043E\u043F\u0443\u043A\u0430\u0439"
    cameraPosition: {x: 5, y: 0, z: -10}
    dialogueText: "\u0418 \u0447\u0442\u043E \u0436\u0435 \u044D\u0442\u043E?"
  - leftCharacterAnim: 2
    rightCharacterAnim: 1
    BackgroundBlurColor: {r: 0.85860395, g: 1, b: 0.10849059, a: 0}
    characterName: "\u042D\u043D\u043D\u0438"
    cameraPosition: {x: -5, y: 0, z: -10}
    dialogueText: "\u041C\u044B \u0434\u0430\u0434\u0438\u043C \u0432\u0430\u043C
      \u0434\u0435\u043D\u044C\u0433\u0438 \u0432 \u0434\u043E\u043B\u0433, \u0430
      \u0432\u044B \u043E\u0442\u0434\u0430\u0434\u0438\u0442\u0435 \u0432 \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E
      \u0440\u0430\u0437 \u0431\u043E\u043B\u044C\u0448\u0435, \u043F\u0440\u0430\u0432\u0434\u0430
      \u0437\u0434\u043E\u0440\u043E\u0432\u043E!"
  - leftCharacterAnim: 1
    rightCharacterAnim: 2
    BackgroundBlurColor: {r: 0.85860395, g: 1, b: 0.10849059, a: 0}
    characterName: "\u0427\u043E\u043F\u0443\u043A\u0430\u0439"
    cameraPosition: {x: 5, y: 0, z: -10}
    dialogueText: "\u0421\u043F\u0430\u0441\u0438\u0431\u043E, \u043D\u0435 \u043D\u0430\u0434\u043E."
